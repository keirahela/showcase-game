				end),
			},

			Blend.New "UICorner" {
				CornerRadius = UDim.new(0.05, 0),
			},
		},
	}))

	local PERIOD = 2
	maid:GiveTask(RunService.RenderStepped:Connect(function()
		isVisible.Value = os.clock() / PERIOD % 1 < 0.5
	end))

	return function()
		maid:DoCleaning()
	end
end
