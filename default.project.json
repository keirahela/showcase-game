{
  "name": "Game",
  "globIgnorePaths": [ "**/.package-lock.json" ],
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "Remotes": {
        "$className": "Folder",
        "CombatEvent": {
          "$className": "RemoteEvent"
        },
        "DeltaCompress": {
          "$className": "RemoteEvent"
        },
        "CombatFunction": {
          "$className": "RemoteFunction"
        }
      }
    },
    "ServerScriptService": {
      "Game": {
        "$className": "Folder",
        "game": {
          "$path": "src/modules"
        },
        "node_modules": {
          "$path": "node_modules"
        }
      },
      "GameScripts": {
        "$path": "src/scripts/Server"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "GameScripts": {
          "$path": "src/scripts/Client"
        }
      }
    }
  }
}